{
    "name": "react-material",
    "version": "0.0.2",
    "description": "Google Material Components for React",
    "main": "lib/index.js",
    "private": true,
    "config": {
        "MOCHA_OPTS": "test/unit/ --opts test/unit/mocha.opts",
        "ISPARTA_OPTS": "--report html",
        "COVERAGE_OPTS": "--statements 100 --functions 100 --branches 100 --lines 100"
    },
    "dependencies": {
        "lodash": "3.10.1",
        "react": "0.14.0",
        "react-addons-pure-render-mixin": "0.14.0",
        "react-intl": "1.2.1",
        "valid-url": "^1.0.9"
    },
    "devDependencies": {
        "babel": "6.3.26",
        "babel-cli": "6.3.17",
        "babel-core": "6.3.26",
        "babel-eslint": "4.1.6",
        "babel-loader": "6.2.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "babel-preset-stage-0": "^6.3.13",
        "babel-runtime": "6.3.19",
        "chai": "^3.2.0",
        "chai-as-promised": "^5.1.0",
        "chai-string": "^1.1.2",
        "chai-subset": "^1.1.0",
        "chai-things": "^0.2.0",
        "css-loader": "^0.17.0",
        "es6-promise": "3.0.2",
        "eslint": "1.2.1",
        "eslint-config-gpbl": "1.0.2",
        "eslint-plugin-mocha": "^0.5.1",
        "eslint-plugin-react": "3.2.3",
        "extract-text-webpack-plugin": "^0.8.2",
        "isparta": "^3.0.4",
        "istanbul": "^0.4.1",
        "jsdom": "^6.3.0",
        "mocha": "^2.3.0",
        "node-dev": "^2.7.1",
        "node-sass": "^3.3.2",
        "react-hot-loader": "1.3.0",
        "react-shallow-testutils": "^0.6.1",
        "run-mode": "^1.0.1",
        "sass-loader": "^2.0.1",
        "sinon": "^1.16.1",
        "sinon-as-promised": "^4.0.0",
        "sinon-chai": "^2.8.0",
        "style-loader": "^0.12.3"
    },
    "scripts": {
        "lint": "eslint src",
        "build": "npm run lint && babel --stage 1 ./src --out-dir ./lib",
        "postinstall": "babel ./src --out-dir ./lib",
        "test": "npm run test:unit:with-coverage",
        "test:unit": "NODE_ENV=test mocha test/unit --opts test/unit/mocha.opts",
        "test:unit:with-coverage": "NODE_ENV=test istanbul cover _mocha test/unit -- --opts test/unit/mocha.opts && istanbul check-coverage"
    }
}
